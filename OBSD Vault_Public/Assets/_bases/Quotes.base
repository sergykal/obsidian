filters:
  and:
    - file.hasTag("quote")
    - file.folder != "Assets"
views:
  - type: table
    name: Table
    order:
      - file.ctime
      - file.name
      - file.folder
    sort:
      - property: file.ctime
        direction: DESC
      - property: file.folder
        direction: ASC
    cardSize: 430
    columnSize:
      file.ctime: 196
      file.name: 500
