filters:
  and:
    - file.hasTag("medical-record")
    - file.folder != "Assets"
views:
  - type: table
    name: Table
    order:
      - file.ctime
      - file.name
      - file.embeds
      - file.backlinks
    sort:
      - property: file.ctime
        direction: ASC
    columnSize:
      file.ctime: 204
      file.name: 560
      file.embeds: 721
